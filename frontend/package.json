{
  "name": "curva-mestra-system-frontend",
  "version": "1.0.0",
  "description": "Curva Mestra System - Frontend Interface",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "test": "vitest --run",
